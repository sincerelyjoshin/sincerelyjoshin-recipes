<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Description</key>
  <string>
    Downloads the latest VEXcode V5 Blocks disk image.
  </string>
  <key>Identifier</key>
  <string>com.github.sincerelyjoshin.download.VEXCodeV5Blocks</string>
  <key>Input</key>
  <dict>
    <key>NAME</key>
    <string>VEXcodeV5Blocks</string>
  </dict>
  <key>MinimumVersion</key>
  <string>0.2.9</string>
  <key>Process</key>
  <array>
    <dict>
      <key>Processor</key>
      <string>URLDownloader</string>
      <key>Arguments</key>
      <dict>
        <key>url</key>
        <string>https://link.vex.com/vexcode-v5blocks-mac</string>
        <key>filename</key>
        <string>%NAME%.dmg</string>
      </dict>
    </dict>
    <dict>
      <key>Processor</key>
      <string>CodeSignatureVerifier</string>
      <key>Arguments</key>
      <dict>
        <key>input_path</key>
        <string>%RECIPE_CACHE_DIR%/downloads/%NAME%.dmg/VEXcode V5.pkg</string>
        <key>expected_authority_names</key>
        <array>
          <string>Developer ID Installer: Vex Robotics, Inc. (3UM3U6335W)</string>
          <string>Developer ID Certification Authority</string>
          <string>Apple Root CA</string>
        </array>
      </dict>
    </dict>
    <dict>
      <key>Processor</key>
      <string>FlatPkgUnpacker</string>
      <key>Arguments</key>
      <dict>
        <key>flat_pkg_path</key>
        <string>%RECIPE_CACHE_DIR%/downloads/%NAME%.dmg/VEXcode V5.pkg</string>
        <key>destination_path</key>
        <string>%RECIPE_CACHE_DIR%/%NAME%</string>
        <key>purge_destination</key>
        <true/>
      </dict>
    </dict>
    <dict>
      <key>Processor</key>
      <string>PkgPayloadUnpacker</string>
      <key>Arguments</key>
      <dict>
        <key>destination_path</key>
        <string>%RECIPE_CACHE_DIR%/payload</string>
        <key>pkg_payload_path</key>
        <string>%RECIPE_CACHE_DIR%/%NAME%/VEXCodeV5.pkg/Payload</string>
        <key>purge_destination</key>
        <true/>
      </dict>
    </dict>
    <dict>
      <key>Processor</key>
      <string>Versioner</string>
      <key>Arguments</key>
      <dict>
        <key>input_plist_path</key>
        <string>%RECIPE_CACHE_DIR%/payload/VEXcode V5.app/Contents/Info.plist</string>
        <key>plist_version_key</key>
        <string>CFBundleVersion</string>
      </dict>
    </dict>
    <dict>
      <key>Processor</key>
      <string>PathDeleter</string>
      <key>Arguments</key>
      <dict>
        <key>path_list</key>
        <array>
          <string>%RECIPE_CACHE_DIR%/payload</string>
          <string>%RECIPE_CACHE_DIR%/%NAME%</string>
        </array>
      </dict>
    </dict>
    <dict>
      <key>Processor</key>
      <string>EndOfCheckPhase</string>
    </dict>
  </array>
</dict>
</plist>
